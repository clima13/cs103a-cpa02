<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Title</td><td>Description</td>
        </tr>
    </thead>
    <tbody>
        <% for (poll of polls) { %>
            <tr>
                <td><a href="poll/<%=poll._id%>"><%=poll.title%></a></td><td><%=poll.description%></td>
            </tr>
        <% } %>
    </tbody>
</table>